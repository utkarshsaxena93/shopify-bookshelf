<% content_for :head do %>
  <title>ShopifyBookshelf</title>
  <%= stylesheet_link_tag    'homepage', media: 'all' %>
<% end %>

<% if current_user %>
  <%= render 'loggedInIndex' %>
<% else %>
  <%= render 'loggedOutIndex' %>
<% end %>

<% content_for :scripts do %>
  <%= javascript_include_tag 'homepage' %>
<% end %>
